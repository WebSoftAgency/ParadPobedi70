<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.websoftagency.patriot20150509" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Patriot</name>
  <description>
        Вооружение и военная тежника парада Победы 2015 года.
    </description>
  <author email="nikolay.wsa@gmail.com" href="http://websoftagency.com/">
      WebSoftAgency Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
